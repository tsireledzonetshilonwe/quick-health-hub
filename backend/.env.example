# Server Configuration
NODE_ENV=development
PORT=8080

# Database (MySQL)
# Example: mysql://<user>:<pass>@localhost:3306/quick_health_hub
DATABASE_URL="mysql://root:password@localhost:3306/quick_health_hub"

# Session Secret (change this in production!)
SESSION_SECRET=your-secret-key-change-this-in-production

# CORS
# Comma-separated allowed frontend origins for CORS
# Example: http://localhost:5173,http://localhost:5175
FRONTEND_URLS=http://localhost:5173,http://localhost:5174,http://localhost:5175

# Session Configuration
SESSION_MAX_AGE=86400000
